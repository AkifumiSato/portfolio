{
  "name": "portofolio",
  "description": "Akifumi Sato Portofolio",
  "private": true,
  "version": "1.0.1",
  "author": "Akifumi Sato",
  "engines": {
    "node": "10.10.x",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "gatsby develop & npm run storybook",
    "dev": "gatsby develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "heroku-postbuild": "gatsby build",
    "storybook": "start-storybook -p 6006",
    "storybook:docker": "start-storybook -p 6006 --ci --quiet",
    "build-storybook": "build-storybook",
    "test": "jest --runInBand",
    "type-check": "tsc",
    "lint": "eslint './**/*.{ts,tsx}'",
    "fix": "eslint './**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.4.0",
    "gatsby": "2.25.3",
    "gatsby-image": "2.4.21",
    "gatsby-link": "2.4.16",
    "gatsby-plugin-google-analytics": "2.4.0",
    "gatsby-plugin-manifest": "2.5.2",
    "gatsby-plugin-offline": "3.3.2",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-sharp": "2.7.1",
    "gatsby-plugin-sitemap": "2.5.1",
    "gatsby-plugin-styled-components": "3.3.14",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-contentful": "3.1.3",
    "gatsby-transformer-remark": "2.9.2",
    "gatsby-transformer-sharp": "2.5.21",
    "immer": "7.0.14",
    "lodash": "4.17.20",
    "prismjs": "1.22.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-redux": "7.2.2",
    "styled-components": "5.2.1",
    "typeface-raleway": "0.0.75"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/preset-typescript": "7.12.1",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-info": "5.3.14",
    "@storybook/addon-links": "5.3.14",
    "@storybook/addon-storyshots": "5.3.14",
    "@storybook/addon-storysource": "5.3.14",
    "@storybook/addon-viewport": "5.3.14",
    "@storybook/addons": "5.3.14",
    "@storybook/react": "5.3.14",
    "@types/jest": "26.0.0",
    "@types/lodash": "4.14.149",
    "@types/prismjs": "1.16.0",
    "@types/react-helmet": "5.0.15",
    "@types/react-redux": "7.1.7",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-mock-store": "1.0.2",
    "@types/storybook__addon-actions": "5.2.1",
    "@types/storybook__addon-info": "5.2.1",
    "@types/storybook__addon-links": "5.2.1",
    "@types/storybook__react": "5.2.1",
    "@types/styled-components": "5.0.1",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-styled-components": "1.10.7",
    "babel-preset-gatsby": "0.2.29",
    "babel-preset-react-app": "9.1.1",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "fetch-mock": "9.10.1",
    "gatsby-plugin-typescript": "2.1.27",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "react-docgen-typescript-loader": "3.6.0",
    "react-test-renderer": "16.13.0",
    "redux-mock-store": "1.5.4",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && jest && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run fix"
    ]
  }
}
